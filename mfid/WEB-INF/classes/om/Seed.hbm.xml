<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.innefu.mfid.dataaccess.om">
	<class
		name="Seed"
		table="seed"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="seed_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Seed"
			column="seed"
			type="string"
			not-null="false"
			length="750"
		/>
		<property
			name="SeedOffset"
			column="seed_offset"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="SeedActiveStatus"
			column="seed_active_status"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="SeedAuthCode"
			column="seed_auth_code"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="SeedType"
			column="seed_type"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="AvailableFlag"
			column="available_flag"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="DeassociateStatus"
			column="deassociate_status"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="LastOtp"
			column="last_otp"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="SecondLastOtp"
			column="second_last_otp"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ErrorCounter"
			column="error_counter"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="ExpiryTime"
			column="expiry_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="LastResyncDate"
			column="last_resync_date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="LockReason"
			column="lock_reason"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="LockDate"
			column="lock_date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="ResyncCounter"
			column="resync_counter"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="Description"
			column="description"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="LastOtpUpdationTime"
			column="last_otp_updation_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="StartTime"
			column="start_time"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="LicenseKey"
			column="license_key"
			type="string"
			not-null="false"
			length="30"
		/>
		<many-to-one
			name="Domain"
			column="domain_id"
			class="Domain"
			not-null="false"
		>
		</many-to-one>
	

		<set name="UserMappings" inverse="true">
			<key column="seed_id"/>
			<one-to-many class="UserMapping"/>
		</set>


	</class>	
</hibernate-mapping>