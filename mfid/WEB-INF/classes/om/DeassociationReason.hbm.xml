<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.innefu.mfid.dataaccess.om">
	<class
		name="DeassociationReason"
		table="deassociation_reason"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="deassociation_reason_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="DeassociationReasonDesc"
			column="deassociation_reason_desc"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>